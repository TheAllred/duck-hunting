    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "knex": "2.4.0",
    "morgan": "^1.10.0",
    "pg": "8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^6.0.0",
    "remixicon-react": "^1.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@types/pg": "8.6.6",
    "@types/react": "^18.0.25",
